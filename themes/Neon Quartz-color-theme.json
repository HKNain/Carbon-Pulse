{
    "name": "Neon Quartz",
    "colors": {
      "editor.background": "#f0eded",
      "editor.foreground": "#000000",
      "sideBarTitle.foreground": "#000000",
      "sideBarTitle.background": "#ffffff",
      "sideBar.background": "#c0c2c3",                 
      "sideBar.foreground": "#1b1a1a",          
      "sideBarSectionHeader.background": "#f3f0f0",
      "sideBarSectionHeader.foreground": "#000000",
      "sideBar.border": "#ffffff",
  
      "list.activeSelectionBackground": "#87878e",
      "list.activeSelectionForeground": "#ffffff",
      "list.hoverBackground": "#4f4e49",
      "list.hoverForeground": "#ffffff",
  
      "activityBar.background": "#d1d3d8",
      "activityBar.foreground": "#000000",
      "activityBar.inactiveForeground": "#636f6f",
      "activityBarBadge.background": "#8f9094",
      "activityBarBadge.foreground": "#000000",
      "activityBar.border": "#444444"
  
    },
    "tokenColors": [
      {
        "name": "Comment",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#56ae44"
        }
      },
      {
        "name": "Variables",
        "scope": ["variable", "string constant.other.placeholder"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#c8541a"
        }
      },
      {
        "scope": ["string", "string.quoted", "string.quoted.double", "string.quoted.single"],
        "settings": {
          "foreground": "#9b6509" // your desired color
        }
      },
      {
        "scope": [
          "constant.numeric",
          "number",
          "constant.numeric.integer",
          "constant.numeric.float"
        ],
        "settings": {
          "foreground": "#b0970a"  
        }
      },
      {
        "name": "Colors",
        "scope": ["constant.other.color"],
        "settings": {
          "foreground": "#000000"
        }
      },
      {
        "name": "Invalid",
        "scope": ["invalid", "invalid.illegal"],
        "settings": {
          "foreground": "#ff0000"
        }
      },
      {
        "name": "Keyword, Storage",
        "scope": ["keyword", "storage.type", "storage.modifier"],
        "settings": {
          "foreground": "#000000"
        }
      },
      {
        "name": "Operator, Misc",
        "scope": [
          "keyword.control",
          "constant.other.color",
          "punctuation",
          "meta.tag",
          "punctuation.definition.tag",
          "punctuation.separator.inheritance.php",
          "punctuation.definition.tag.html",
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html",
          "punctuation.section.embedded",
          "keyword.other.template",
          "keyword.other.substitution"
        ],
        "settings": {
          "foreground": "#000000"
        }
      },
      {
        "name": "Tag",
        "scope": [
          "entity.name.tag",
          "meta.tag.sgml",
          "markup.deleted.git_gutter"
        ],
        "settings": {
          "foreground": "#064bec"
        }
      },
      {
        "name": "Function, Special Method",
        "scope": [
          "entity.name.function",
          "meta.function-call",
          "variable.function",
          "support.function",
          "keyword.other.special-method"
        ],
        "settings": {
          "foreground": "#4a4c12"
        }
      },
      {
        "name": "Block Level Variables",
        "scope": ["meta.block variable.other"],
        "settings": {
          "foreground": "#f07178"
        }
      },
      {
        "name": "Other Variable, String Link",
        "scope": ["support.other.variable", "string.other.link"],
        "settings": {
          "foreground": "#f07178"
        }
      },
      {
        "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
        "scope": [
          "constant.numeric",
          "constant.language",
          "support.constant",
          "constant.character",
          "constant.escape",
          "variable.parameter",
          "keyword.other.unit",
          "keyword.other"
        ],
        "settings": {
          "foreground": "#F78C6C"
        }
      },
      {
        "name": "String, Symbols, Inherited Class, Markup Heading",
        "scope": [
          "string",
          "constant.other.symbol",
          "constant.other.key",
          "entity.other.inherited-class",
          "markup.heading",
          "markup.inserted.git_gutter",
          "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
        ],
        "settings": {
          "foreground": "#624b14"
        }
      },
      {
        "name": "Class, Support",
        "scope": [
          "entity.name",
          "support.type",
          "support.class",
          "support.other.namespace.use.php",
          "meta.use.php",
          "support.other.namespace.php",
          "markup.changed.git_gutter",
          "support.type.sys-types"
        ],
        "settings": {
          "foreground": "#71531a"
        }
      },
      {
        "name": "Entity Types",
        "scope": ["support.type"],
        "settings": {
          "foreground": "#0e516b"
        }
      },
      {
        "name": "CSS Class and Support",
        "scope": [
          "source.css support.type.property-name",
          "source.sass support.type.property-name",
          "source.scss support.type.property-name",
          "source.less support.type.property-name",
          "source.stylus support.type.property-name",
          "source.postcss support.type.property-name"
        ],
        "settings": {
          "foreground": "#044057"
        }
      },
      {
        "name": "Sub-methods",
        "scope": [
          "entity.name.module.js",
          "variable.import.parameter.js",
          "variable.other.class.js"
        ],
        "settings": {
          "foreground": "#48620d"
        }
      },
      {
        "name": "Language methods",
        "scope": ["variable.language"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#FF5370"
        }
      },
      {
        "name": "entity.name.method.js",
        "scope": ["entity.name.method.js"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#b33535"
        }
      },
      {
        "name": "meta.method.js",
        "scope": [
          "meta.class-method.js entity.name.function.js",
          "variable.function.constructor"
        ],
        "settings": {
          "foreground": "#1d3360"
        }
      },
      {
        "name": "Attributes",
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "foreground": "#761773"
        }
      },
      {
        "name": "HTML Attributes",
        "scope": [
          "text.html.basic entity.other.attribute-name.html",
          "text.html.basic entity.other.attribute-name"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#621712"
        }
      },
      {
        "name": "CSS Classes",
        "scope": ["entity.other.attribute-name.class"],
        "settings": {
          "foreground": "#5c4519"
        }
      },
      {
        "name": "CSS ID's",
        "scope": ["source.sass keyword.control"],
        "settings": {
          "foreground": "#4f3107"
        }
      },
      {
        "name": "Inserted",
        "scope": ["markup.inserted"],
        "settings": {
          "foreground": "#5c8420"
        }
      },
      {
        "name": "Deleted",
        "scope": ["markup.deleted"],
        "settings": {
          "foreground": "#cf1f1f"
        }
      },
      {
        "name": "Changed",
        "scope": ["markup.changed"],
        "settings": {
          "foreground": "#651d95"
        }
      },
      {
        "name": "Regular Expressions",
        "scope": ["string.regexp"],
        "settings": {
          "foreground": "#256179"
        }
      },
      {
        "name": "Escape Characters",
        "scope": ["constant.character.escape"],
        "settings": {
          "foreground": "#000000"
        }
      },
      {
        "name": "URL",
        "scope": ["*url*", "*link*", "*uri*"],
        "settings": {
          "fontStyle": "underline",
        }
      },
      {
        "name": "Decorators",
        "scope": [
          "tag.decorator.js entity.name.tag.js",
          "tag.decorator.js punctuation.definition.tag.js"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#22458f"
        }
      },
      {
        "name": "ES7 Bind Operator",
        "scope": [
          "source.js constant.other.object.key.js string.unquoted.label.js"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#8c1529"
        }
      },
